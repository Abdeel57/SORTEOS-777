{
  "name": "lucky-snap-backend",
  "version": "0.0.1",
  "description": "Backend for Lucky Snap",
  "private": true,
  "scripts": {
    "build": "npx prisma generate && nest build",
    "build:with-migrate": "npx prisma migrate deploy && npx prisma generate && nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "postinstall": "npx prisma generate",
    "postbuild": "npm run generate",
    "generate": "npx prisma generate",
    "migrate:status": "npx prisma migrate status",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:data": "node migrate-data.js",
    "test:db": "node test-database.js",
    "backup": "node backup-data.js",
    "typecheck": "npx tsc --noEmit",
    "add-indexes": "ts-node scripts/add-indexes.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.17.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.1.3",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.17",
    "@types/node": "^20.19.24",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "prisma": "^5.22.0",
    "sqlite3": "^5.1.7",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2"
  }
}
